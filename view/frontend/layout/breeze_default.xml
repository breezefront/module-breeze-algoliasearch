<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="better_compatibility" xsi:type="array">
                    <item name="Algolia_AlgoliaSearch" xsi:type="boolean">false</item>
                </argument>
                <argument name="ignore" xsi:type="array">
                    <item name="algoliaHooks" xsi:type="string">algoliaHooks</item>
                </argument>
                <argument name="bundles" xsi:type="array">
                    <item name="algolia" xsi:type="array">
                        <item name="sort_order" xsi:type="number">90</item>
                        <item name="items" xsi:type="array">
                            <item name="algoliaSearchLib" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/lib/algolia-search.min.js</item>
                                <item name="global" xsi:type="string">algoliasearch</item>
                            </item>
                            <item name="algoliaInstantSearchLib" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/lib/algolia-instantsearch.min.js</item>
                                <item name="global" xsi:type="string">instantsearch</item>
                            </item>
                            <item name="algoliaAutocompleteLib" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/lib/algolia-autocomplete.min.js</item>
                                <item name="global" xsi:type="string">@algolia/autocomplete-js</item>
                            </item>
                            <item name="algoliaAnalyticsLib" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/lib/search-insights.min.js</item>
                                <item name="global" xsi:type="string">AlgoliaAnalytics</item>
                            </item>
                            <item name="algoliaQuerySuggestionsPluginLib" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/lib/query-suggestions-plugin.min.js</item>
                                <item name="global" xsi:type="string">@algolia/autocomplete-plugin-query-suggestions</item>
                            </item>
                            <item name="algoliaMustacheLib" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/lib/mustache.min.js</item>
                                <item name="global" xsi:type="string">Mustache</item>
                            </item>
                            <item name="algoliaBase64" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/internals/base64</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="algoliaTemplateEngine" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/internals/template-engine</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="algoliaAutocompleteProductsHtml" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/template/autocomplete/products</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="algoliaAutocompleteCategoriesHtml" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/template/autocomplete/categories</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="algoliaAutocompletePagesHtml" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/template/autocomplete/pages</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="algoliaAutocompleteSuggestionsHtml" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/template/autocomplete/suggestions</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="algoliaAutocompleteAdditionalHtml" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/template/autocomplete/additional-section</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="algoliaCommon" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/internals/common</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="algoliaInsights" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/insights</item>
                                <item name="global" xsi:type="string">algoliaInsights</item>
                            </item>
                            <item name="algoliaHooks" xsi:type="array">Algolia_AlgoliaSearch/js/hooks</item>
                            <item name="algoliaAutocomplete" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/autocomplete</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="algoliaInstantSearch" xsi:type="array">
                                <item name="path" xsi:type="string">Algolia_AlgoliaSearch/js/instantsearch</item>
                                <item name="anonymous" xsi:type="boolean">true</item>
                            </item>
                            <item name="algoliaMixins" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAlgoliasearch/js/mixins</item>
                                <item name="mixins" xsi:type="array">
                                    <item name="addToCart" xsi:type="string">catalogAddToCart</item>
                                </item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
